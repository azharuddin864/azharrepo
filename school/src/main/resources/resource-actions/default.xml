<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.2.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_2_0.dtd">

<resource-action-mapping>
<portlet-resource>
         <portlet-name>com_hp_SchoolPortlet</portlet-name>
         <permissions>
            <supports>               
                <action-key>VIEW</action-key>
                <action-key>ADD_STUDENT</action-key>
                <action-key>UPDATE_STUDENT</action-key>
                <action-key>DELETE_STUDENT</action-key>
                <action-key>ADD_TO_PAGE</action-key>
           </supports>
            <community-defaults/>
            <guest-defaults>
                <action-key>VIEW</action-key>
            </guest-defaults>
            <guest-unsupported>               
                <action-key>DELETE_STUDENT</action-key>
            </guest-unsupported>
        </permissions>
</portlet-resource>
</resource-action-mapping>


